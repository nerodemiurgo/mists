{
    "name": "luoning/htmlentities",
    "type": "phpbb-extension",
    "description": "Render HTML entities in post text content",
    "homepage": "https://github.com/lionel-rowe/phpbb-html-entities",
    "version": "1.0.0",
    "time": "2022-03-09",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Lionel Rowe",
            "homepage": "https://github.com/lionel-rowe"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
    },
    "extra": {
        "display-name": "HTML Entities",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/html_entities",
            "filename": "version_check",
            "ssl": true
        }
    }
}